{
  "name": "mongodbexpressreactnodeqoejr",
  "version": "1.0.0",
  "description": "A generated IBM Cloud application",
  "private": true,
  "engines": {
    "node": "^6.9.0"
  },
  "watch": {
    "test": {
      "patterns": [
        "src/**",
        "server/**"
      ],
      "extensions": "js,jsx",
      "quiet": true,
      "delay": 2500
    }
  },
  "scripts": {
    "start": "node server/server.js",
    "debug": "node --debug server/server.js",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "dev": "nf --procfile Procfile-dev --port 3000 start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "client-reload": "react-scripts start",
    "server-reload": "nodemon server/server.js",
    "lint": "eslint --ext .js --ext .jsx --ignore-path .gitignore  .",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "watch": "npm-watch | grep --invert-match nodemon"
  },
  "dependencies": {
    "appmetrics-dash": "^3.3.2",
    "appmetrics-prometheus": "^0.0.2",
    "axios": "^0.16.1",
    "body-parser": "^1.17.1",
    "bulma": "^0.4.1",
    "cfenv": "^1.0.4",
    "connect-mongo": "^1.3.2",
    "express": "^4.15.3",
    "express-session": "^1.15.2",
    "foreman": "^2.0.0",
    "ibm-cloud-env": "^0",
    "log4js": "^1.1.1",
    "mongoose": "^4.9.8",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.1",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-watch": "^3.1.3",
    "nodemon": "^1.12.1",
    "npm-watch": "^0.3.0",
    "nyc": "^10.3.2",
    "react-scripts": "^0.9.5",
    "sinon": "^4.2.2"
  },
  "proxy": "http://localhost:3200"
}
